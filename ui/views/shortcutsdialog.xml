<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkShortcutsWindow" id="gtk_shortcutsDialog">
        <property name="default-width">600</property>
        <property name="default-height">500</property>
        <property name="modal">true</property>
        <property name="destroy-with-parent">false</property>
        <property name="hide-on-close">true</property>
        <child>
            <object class="GtkShortcutsSection">
                <child>
                    <object class="GtkShortcutsGroup">
                        <property name="title">Tube Converter</property>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">Select Save Folder</property>
                                <property name="accelerator">&lt;Control&gt;o</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">Download Videos</property>
                                <property name="accelerator">&lt;Control&gt;d</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkShortcutsGroup">
                        <property name="title">Downloads Queue</property>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">Add To Queue</property>
                                <property name="accelerator">&lt;Control&gt;&lt;Shift&gt;a</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">Remove From Queue</property>
                                <property name="accelerator">Delete</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">Clear Queue</property>
                                <property name="accelerator">&lt;Control&gt;&lt;Shift&gt;c</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkShortcutsGroup">
                        <property name="title">Application</property>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="title">About</property>
                                <property name="accelerator">F1</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
